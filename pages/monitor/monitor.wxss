/* pages/monitor/monitor.wxss */

/* 页面容器 */
.container {
  min-height: 100vh;
  padding-top: calc(var(--status-bar-height) + 20rpx);
  padding-left: 20rpx;
  padding-right: 20rpx;
  padding-bottom: calc(140px + env(safe-area-inset-bottom) + 40rpx); /* 底部导航栏高度 + 安全区域 + 额外间距 */
  box-sizing: border-box;
  transition: background 0.3s ease;
}

/* 动态背景 */
.container-default,
.container-girl,
.container-female {
  background: linear-gradient(135deg, #ffeef8 0%, #ffe0f0 100%);
}

.container-boy,
.container-male {
  background: linear-gradient(135deg, #e6f3ff 0%, #ccebff 100%);
}

/* 主卡片容器 */
.main-card {
  width: 100%;
  background: white;
  border-radius: 24rpx;
  box-shadow: 0 8rpx 24rpx rgba(0,0,0,0.08);
  overflow: visible; /* 改为visible，确保调试区域可见 */
  margin-bottom: 20rpx;
}

/* 顶部欢迎区域 */
.welcome-section {
  padding: 30rpx 24rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  transition: background 0.3s ease;
}

.welcome-section.container-default,
.welcome-section.container-girl,
.welcome-section.container-female {
  background: linear-gradient(135deg, #ff6b9d 0%, #ff8fab 100%);
}

.welcome-section.container-boy,
.welcome-section.container-male {
  background: linear-gradient(135deg, #4facfe 0%, #00b4db 100%);
}

.welcome-content {
  display: flex;
  flex-direction: column;
  gap: 4rpx;
  flex: 1;
}

.welcome-title {
  color: white;
  font-size: 30rpx;
  font-weight: 600;
}

.welcome-subtitle {
  color: rgba(255, 255, 255, 0.8);
  font-size: 22rpx;
}

.action-buttons {
  display: flex;
  gap: 12rpx;
  align-items: center;
}

.refresh-btn {
  padding: 12rpx 20rpx;
  background: rgba(255, 255, 255, 0.25);
  border-radius: 24rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  transition: all 0.3s ease;
  border: 1rpx solid rgba(255, 255, 255, 0.3);
  min-width: 60rpx;
}

.refresh-btn:active {
  background: rgba(255, 255, 255, 0.4);
  transform: scale(0.95);
  box-shadow: 0 2rpx 8rpx rgba(0, 0, 0, 0.1);
}

.refresh-icon {
  color: white;
  font-size: 22rpx;
  font-weight: 600;
  text-shadow: 0 1rpx 2rpx rgba(0, 0, 0, 0.1);
}

/* 成长监测模块 */
.growth-module {
  padding: 16rpx;
  border-bottom: 1rpx solid #f1f5f9;
}

.module-header {
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-bottom: 12rpx;
}

.module-title {
  font-size: 32rpx;
  font-weight: 600;
  color: #1a202c;
}

/* 宝宝简介卡片样式 */
.baby-profile-card {
  margin-bottom: 12rpx;
  background: linear-gradient(135deg, #ffffff 0%, #f8fafc 100%);
  border-radius: 16rpx;
  padding: 16rpx;
  box-shadow: 0 4rpx 16rpx rgba(0, 0, 0, 0.06);
  border: 1rpx solid rgba(255, 255, 255, 0.8);
  display: flex;
  gap: 16rpx;
}

/* 左侧基本信息 */
.profile-left {
  display: flex;
  gap: 16rpx;
  flex: 1;
}

.baby-avatar {
  width: 64rpx;
  height: 64rpx;
  border-radius: 32rpx;
  background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
  display: flex;
  align-items: center;
  justify-content: center;
  flex-shrink: 0;
}

.avatar-text {
  font-size: 28rpx;
  font-weight: 700;
  color: white;
}

.baby-basic-info {
  display: flex;
  flex-direction: column;
  justify-content: center;
  gap: 6rpx;
  flex: 1;
}

.baby-name {
  font-size: 28rpx;
  font-weight: 700;
  color: #1a202c;
}

.baby-details {
  display: flex;
  gap: 12rpx;
}

.baby-gender,
.baby-age {
  font-size: 20rpx;
  color: #64748b;
  background: rgba(0, 0, 0, 0.05);
  padding: 4rpx 8rpx;
  border-radius: 8rpx;
  font-weight: 500;
}

.last-update {
  font-size: 18rpx;
  color: #94a3b8;
}

/* 右侧指标数据 */
.profile-right {
  flex: 1;
}

.metrics-grid {
  display: flex;
  flex-direction: column;
  gap: 8rpx;
  height: 100%;
}

.metric-item {
  flex: 1;
  background: #f8fafc;
  border-radius: 12rpx;
  padding: 12rpx 16rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border: 1rpx solid rgba(0, 0, 0, 0.05);
  transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1);
  cursor: pointer;
}

.metric-item:active {
  transform: scale(0.95);
}

.metric-item.active {
  background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
  transform: scale(1.02);
  box-shadow: 0 4rpx 12rpx rgba(102, 126, 234, 0.25);
}

.metric-item.active .metric-label,
.metric-item.active .metric-value {
  color: white;
}

.metric-label {
  font-size: 20rpx;
  color: #64748b;
  font-weight: 600;
}

.metric-value {
  font-size: 22rpx;
  font-weight: 600;
  color: #1a202c;
}

/* 女宝宝主题 */
.container-girl .baby-profile-card,
.container-female .baby-profile-card {
  background: linear-gradient(135deg, #fff5f8 0%, #ffeef5 100%);
  border: 1rpx solid rgba(255, 182, 193, 0.3);
  box-shadow: 0 8rpx 24rpx rgba(255, 105, 157, 0.08);
}

.container-girl .baby-avatar,
.container-female .baby-avatar {
  background: linear-gradient(135deg, #ff6b9d 0%, #ff8fab 100%);
}

.container-girl .metric-item,
.container-female .metric-item {
  background: #fdf2f8;
  border: 1rpx solid rgba(255, 182, 193, 0.2);
}

.container-girl .metric-item.active,
.container-female .metric-item.active {
  background: linear-gradient(135deg, #ff6b9d 0%, #ff8fab 100%);
  box-shadow: 0 4rpx 12rpx rgba(255, 107, 157, 0.25);
}

/* 男宝宝主题 */
.container-boy .baby-profile-card,
.container-male .baby-profile-card {
  background: linear-gradient(135deg, #f0f8ff 0%, #e6f3ff 100%);
  border: 1rpx solid rgba(79, 172, 254, 0.3);
  box-shadow: 0 8rpx 24rpx rgba(79, 172, 254, 0.08);
}

.container-boy .baby-avatar,
.container-male .baby-avatar {
  background: linear-gradient(135deg, #4facfe 0%, #00b4db 100%);
}

.container-boy .metric-item,
.container-male .metric-item {
  background: #f0f9ff;
  border: 1rpx solid rgba(79, 172, 254, 0.2);
}

.container-boy .metric-item.active,
.container-male .metric-item.active {
  background: linear-gradient(135deg, #4facfe 0%, #00b4db 100%);
  box-shadow: 0 4rpx 12rpx rgba(79, 172, 254, 0.25);
}



.section-header {
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-bottom: 16rpx;
}

.section-title {
  font-size: 28rpx;
  font-weight: 600;
  color: #1a202c;
}

.chart-controls {
  display: flex;
  align-items: center;
}

.time-selector {
  display: flex;
  gap: 6rpx;
  background: #f8fafc;
  border-radius: 16rpx;
  padding: 4rpx;
}

.time-option {
  padding: 8rpx 14rpx;
  border-radius: 12rpx;
  font-size: 20rpx;
  color: #64748b;
  transition: all 0.3s ease;
  min-width: 50rpx;
  text-align: center;
}

.time-option.active {
  background: white;
  color: #667eea;
  box-shadow: 0 2rpx 8rpx rgba(0,0,0,0.1);
}

/* 女宝宝主题的时间选择器 */
.container-girl .time-option.active ,
.container-female .time-option.active {
  color: #ff6b9d;
}

/* 男宝宝主题的时间选择器 */
.container-boy .time-option.active ,
.container-male .time-option.active {
  color: #4facfe;
}

/* 图表标题区域 */
.chart-title-section {
  text-align: center;
  margin: 20rpx 0 16rpx 0;
  padding: 16rpx 0;
}

.chart-main-title {
  display: block;
  font-size: 36rpx;
  font-weight: 700;
  color: #1a202c;
  margin-bottom: 8rpx;
  background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  background-clip: text;
}

.chart-subtitle {
  display: block;
  font-size: 24rpx;
  color: #64748b;
  font-weight: 500;
}

/* 女宝宝主题的图表标题 */
.container-girl .chart-main-title,
.container-female .chart-main-title {
  background: linear-gradient(135deg, #ff6b9d 0%, #ff8fab 100%);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  background-clip: text;
}

/* 男宝宝主题的图表标题 */
.container-boy .chart-main-title,
.container-male .chart-main-title {
  background: linear-gradient(135deg, #4facfe 0%, #00b4db 100%);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  background-clip: text;
}

/* 图表区域样式保留供其他部分使用 */

.chart-container {
  background: linear-gradient(135deg, #ffffff 0%, #f8fafc 100%);
  border-radius: 20rpx;
  padding: 24rpx;
  margin: 16rpx 0 24rpx 0;
  box-shadow: 
    0 8rpx 32rpx rgba(0, 0, 0, 0.08),
    0 4rpx 16rpx rgba(0, 0, 0, 0.04);
  border: 1rpx solid rgba(255, 255, 255, 0.8);
}

/* 女宝宝主题的图表容器 */
.container-girl .chart-container ,
.container-female .chart-container {
  background: linear-gradient(135deg, #fff5f8 0%, #ffeef5 100%);
  border: 1rpx solid rgba(255, 182, 193, 0.3);
  box-shadow: 
    0 8rpx 32rpx rgba(255, 105, 157, 0.12),
    0 4rpx 16rpx rgba(255, 105, 157, 0.08);
}

/* 男宝宝主题的图表容器 */
.container-boy .chart-container ,
.container-male .chart-container {
  background: linear-gradient(135deg, #f0f8ff 0%, #e6f3ff 100%);
  border: 1rpx solid rgba(79, 172, 254, 0.3);
  box-shadow: 
    0 8rpx 32rpx rgba(79, 172, 254, 0.12),
    0 4rpx 16rpx rgba(79, 172, 254, 0.08);
}

.chart-canvas {
  width: 100%;
  height: 400rpx;
  background: white;
  border-radius: 16rpx;
  box-shadow: 
    inset 0 2rpx 8rpx rgba(0, 0, 0, 0.03),
    0 4rpx 16rpx rgba(0, 0, 0, 0.06);
  border: 2rpx solid #f1f5f9;
  transition: all 0.3s ease;
}

.chart-canvas:hover {
  transform: scale(1.01);
  box-shadow: 
    inset 0 2rpx 8rpx rgba(0, 0, 0, 0.03),
    0 8rpx 24rpx rgba(0, 0, 0, 0.1);
}

.chart-description {
  margin-top: 16rpx;
  text-align: center;
}

.chart-desc-text {
  font-size: 22rpx;
  color: #64748b;
  background: rgba(102, 126, 234, 0.1);
  padding: 12rpx 16rpx;
  border-radius: 12rpx;
  display: inline-block;
  font-weight: 500;
}

/* 女宝宝主题的图表描述 */
.container-girl .chart-desc-text ,
.container-female .chart-desc-text {
  background: rgba(255, 107, 157, 0.1);
  color: #be185d;
}

/* 男宝宝主题的图表描述 */
.container-boy .chart-desc-text ,
.container-male .chart-desc-text {
  background: rgba(79, 172, 254, 0.1);
  color: #0369a1;
}

/* 记录区域 */
.records-section {
  padding: 12rpx 16rpx;
  border-bottom: 1rpx solid #f1f5f9;
}

.section-action {
  font-size: 24rpx;
  color: #667eea;
  font-weight: 500;
}

/* 女宝宝主题的操作按钮 */
.container-girl .section-action ,
.container-female .section-action {
  color: #ff6b9d;
}

/* 男宝宝主题的操作按钮 */
.container-boy .section-action ,
.container-male .section-action {
  color: #4facfe;
}

.records-list {
  margin-top: 8rpx;
}

.record-item {
  display: flex;
  align-items: center;
  gap: 16rpx;
  padding: 12rpx 16rpx;
  background: #f8fafc;
  border-radius: 10rpx;
  margin-bottom: 8rpx;
  border-left: 4rpx solid #667eea;
  transition: all 0.3s ease;
}

/* 女宝宝主题的记录项 */
.container-girl .record-item ,
.container-female .record-item {
  background: #fdf2f8;
  border-left: 4rpx solid #ff6b9d;
}

.container-girl .record-item:active ,
.container-female .record-item:active {
  background: #fce7f3;
}

/* 男宝宝主题的记录项 */
.container-boy .record-item ,
.container-male .record-item {
  background: #f0f9ff;
  border-left: 4rpx solid #4facfe;
}

.container-boy .record-item:active ,
.container-male .record-item:active {
  background: #e0f2fe;
}

.record-item:active {
  transform: scale(0.98);
  background: #e2e8f0;
}

.record-item:last-child {
  margin-bottom: 0;
}

.record-date {
  min-width: 100rpx;
}

.date-text {
  font-size: 22rpx;
  color: #4a5568;
  font-weight: 500;
}

.record-measurements {
  flex: 1;
  display: flex;
  flex-wrap: wrap;
  gap: 8rpx;
}

.measurement-tag {
  display: flex;
  flex-direction: column;
  align-items: center;
  gap: 2rpx;
  padding: 8rpx 12rpx;
  background: white;
  border-radius: 8rpx;
  border: 1rpx solid #e2e8f0;
  min-width: 80rpx;
}

.tag-label {
  font-size: 18rpx;
  color: #718096;
}

.tag-value {
  font-size: 20rpx;
  font-weight: 600;
  color: #2d3748;
}

/* 空状态 */
.empty-state {
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 40rpx 20rpx;
  text-align: center;
}

.empty-text {
  font-size: 28rpx;
  color: #4a5568;
  font-weight: 500;
  margin-bottom: 8rpx;
}

.empty-desc {
  font-size: 22rpx;
  color: #718096;
  line-height: 1.5;
}



/* 弹窗样式 */
.modal-overlay {
  position: fixed;
  top: 0;
  left: 0;
  width: 100vw;
  height: 100vh;
  background: rgba(0, 0, 0, 0.7);
  display: flex;
  align-items: center;
  justify-content: center;
  z-index: 99999;
  backdrop-filter: blur(4px);
  animation: fade-in 0.3s ease;
}

.modal-content {
  width: 85%;
  max-width: 650rpx;
  background: white;
  border-radius: 32rpx;
  overflow: hidden;
  animation: slide-up 0.3s ease;
  position: relative;
  z-index: 100000;
  box-shadow: 0 20rpx 60rpx rgba(0, 0, 0, 0.4);
  border: 4rpx solid white;
}

@keyframes slide-up {
  from { 
    transform: translateY(100rpx);
    opacity: 0;
  }
  to { 
    transform: translateY(0);
    opacity: 1;
  }
}

@keyframes modalShow {
  from {
    opacity: 0;
    transform: scale(0.8) translateY(-100rpx);
  }
  to {
    opacity: 1;
    transform: scale(1) translateY(0);
  }
}

.modal-header {
  padding: 32rpx;
  border-bottom: 2rpx solid #f1f5f9;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.modal-title {
  font-size: 32rpx;
  font-weight: 600;
  color: #1a202c;
}

.modal-close {
  font-size: 44rpx;
  color: #9ca3af;
  width: 56rpx;
  height: 56rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 50%;
  transition: all 0.3s ease;
}

.modal-close:active {
  background: #f1f5f9;
}

.modal-body {
  padding: 32rpx;
  background: white;
}

.form-group {
  margin-bottom: 24rpx;
}

.form-label {
  display: block;
  font-size: 26rpx;
  color: #4a5568;
  font-weight: 600;
  margin-bottom: 16rpx;
}

.form-input {
  width: 100%;
  padding: 24rpx;
  border: 3rpx solid #e5e7eb;
  border-radius: 20rpx;
  font-size: 28rpx;
  background: #f8fafc;
  transition: all 0.3s ease;
  box-sizing: border-box;
}

.form-input:focus {
  border-color: #667eea;
  background: white;
}

/* 女宝宝主题的输入框焦点 */
.container-girl .form-input:focus ,
.container-female .form-input:focus {
  border-color: #ff6b9d;
}

/* 男宝宝主题的输入框焦点 */
.container-boy .form-input:focus ,
.container-male .form-input:focus {
  border-color: #4facfe;
}

.picker-input {
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.picker-text {
  color: #2d3748;
}

.picker-arrow {
  font-size: 28rpx;
}

.modal-footer {
  padding: 32rpx;
  border-top: 2rpx solid #f1f5f9;
  display: flex;
  gap: 20rpx;
}

.modal-btn {
  flex: 1;
  padding: 28rpx;
  border-radius: 20rpx;
  font-size: 28rpx;
  font-weight: 600;
  border: none;
  transition: all 0.3s ease;
}

.modal-btn-cancel {
  background: #f1f5f9;
  color: #64748b;
}

.modal-btn-cancel:active {
  background: #e2e8f0;
}

.modal-btn-confirm {
  background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
  color: white;
}

/* 女宝宝主题的确认按钮 */
.container-girl .modal-btn-confirm ,
.container-female .modal-btn-confirm {
  background: linear-gradient(135deg, #ff6b9d 0%, #ff8fab 100%);
}

/* 男宝宝主题的确认按钮 */
.container-boy .modal-btn-confirm ,
.container-male .modal-btn-confirm {
  background: linear-gradient(135deg, #4facfe 0%, #00b4db 100%);
}

.modal-btn-confirm:active {
  opacity: 0.8;
}

/* 响应式适配 */
@media (max-width: 480rpx) {
  .container {
    padding: 16rpx 16rpx 40rpx;
  }
  
  .modal-content {
    width: 90%;
  }
  
  .metrics-grid {
    flex-direction: column;
    gap: 8rpx;
  }
  
  .chart-type-tabs {
    flex-direction: column;
    gap: 6rpx;
  }
  
  .chart-type-tab {
    flex-direction: row;
    justify-content: center;
  }
  
  .record-item {
    flex-direction: column;
    align-items: flex-start;
    gap: 12rpx;
  }
}

/* 调试区域样式 */
.debug-section {
  margin-top: 24rpx;
  margin-bottom: 40rpx; /* 增加底部外边距 */
  padding: 32rpx; /* 增加内边距 */
  background: linear-gradient(135deg, #f8fafc 0%, #e2e8f0 100%);
  border-radius: 16rpx;
  border: 2rpx dashed #cbd5e0;
  box-shadow: 0 4rpx 12rpx rgba(0, 0, 0, 0.05);
}

.debug-header {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 20rpx;
}

.debug-title {
  font-size: 28rpx;
  font-weight: 600;
  color: #4a5568;
}

.debug-help {
  font-size: 24rpx;
  color: #667eea;
  padding: 8rpx 16rpx;
  background: rgba(102, 126, 234, 0.1);
  border-radius: 20rpx;
  border: 1rpx solid rgba(102, 126, 234, 0.2);
}

.debug-buttons {
  display: flex;
  flex-direction: column;
  gap: 16rpx;
}

.debug-btn {
  height: 88rpx; /* 增加按钮高度 */
  font-size: 28rpx; /* 增加字体大小 */
  font-weight: 600;
  border: none;
  border-radius: 12rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  transition: all 0.3s ease;
  box-shadow: 0 2rpx 8rpx rgba(0, 0, 0, 0.1);
}

.debug-btn:active {
  transform: scale(0.98);
  box-shadow: 0 1rpx 4rpx rgba(0, 0, 0, 0.15);
}

.debug-btn-primary {
  background: linear-gradient(135deg, #48bb78 0%, #38a169 100%);
  color: white;
}

.debug-btn-info {
  background: linear-gradient(135deg, #4299e1 0%, #3182ce 100%);
  color: white;
}

.debug-btn-warning {
  background: linear-gradient(135deg, #ed8936 0%, #dd6b20 100%);
  color: white;
}

.debug-btn-danger {
  background: linear-gradient(135deg, #f56565 0%, #e53e3e 100%);
  color: white;
}

.debug-btn-success {
  background: linear-gradient(135deg, #68d391 0%, #48bb78 100%);
  color: white;
}

.debug-btn-secondary {
  background: linear-gradient(135deg, #a0aec0 0%, #718096 100%);
  color: white;
}

/* 弹窗显示时隐藏页面内容 */
.container.modal-active {
  visibility: hidden;
}

.container.modal-active .modal-overlay {
  visibility: visible;
}

/* 添加记录弹窗样式覆盖 */
.modal-overlay .modal-content {
  animation: slide-up 0.3s ease;
}

.modal-overlay .modal-body {
  padding: 50rpx 40rpx 30rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  background: white;
}

.modal-overlay .modal-footer {
  padding: 30rpx 40rpx 40rpx;
  display: flex;
  gap: 24rpx;
  border-top: none;
}

.modal-overlay .form-group {
  margin-bottom: 30rpx;
  width: 100%;
  max-width: 400rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.modal-overlay .form-label {
  display: block;
  font-size: 28rpx;
  font-weight: 600;
  color: #374151;
  margin-bottom: 16rpx;
  text-align: center;
  width: 100%;
}

.modal-overlay .form-input {
  width: 100%;
  height: 88rpx;
  background: #f8fafc;
  border: 2rpx solid #e2e8f0;
  border-radius: 16rpx;
  padding: 0 24rpx;
  font-size: 28rpx;
  color: #1a202c;
  box-sizing: border-box;
  transition: all 0.3s ease;
  position: relative;
  z-index: 1;
}

.modal-overlay .form-input:focus {
  border-color: #667eea;
  background: white;
  box-shadow: 0 0 0 6rpx rgba(102, 126, 234, 0.1);
  outline: none;
}

.modal-overlay .picker-input {
  display: flex;
  align-items: center;
  justify-content: space-between;
  position: relative;
  cursor: pointer;
}

.modal-overlay .picker-input:active {
  background: #f8fafc;
  border-color: #667eea;
}

.modal-overlay .picker-text {
  flex: 1;
  color: #1a202c;
}

.modal-overlay .picker-arrow {
  font-size: 28rpx;
  color: #667eea;
  margin-left: 20rpx;
}

.modal-overlay .btn {
  flex: 1;
  height: 88rpx;
  border-radius: 20rpx;
  font-size: 30rpx;
  font-weight: 600;
  display: flex;
  align-items: center;
  justify-content: center;
  transition: all 0.3s ease;
  border: none;
}

.modal-overlay .btn-cancel {
  background: #f3f4f6;
  color: #6b7280;
}

.modal-overlay .btn-cancel:active {
  background: #e5e7eb;
  transform: translateY(2rpx);
}

.modal-overlay .btn-primary {
  background: linear-gradient(135deg, #ff6b9d 0%, #ff8fab 100%);
  color: white;
  box-shadow: 0 8rpx 25rpx rgba(255, 107, 157, 0.3);
}

.modal-overlay .btn-primary:active {
  transform: translateY(2rpx);
  box-shadow: 0 4rpx 15rpx rgba(255, 107, 157, 0.4);
}

.modal-overlay .btn:disabled {
  opacity: 0.6;
  transform: none !important;
  box-shadow: none !important;
} 